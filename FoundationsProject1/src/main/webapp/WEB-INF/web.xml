<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
    <display-name>FoundationsProject1</display-name>


    <servlet>
        <servlet-name>employee</servlet-name>
        <servlet-class>com.SamPage.controller.EmployeeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>employee</servlet-name>
        <url-pattern>/employee/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>management</servlet-name>
        <servlet-class>com.SamPage.controller.ManagementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>management</servlet-name>
        <url-pattern>/management/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>reimburse</servlet-name>
        <servlet-class>com.SamPage.controller.TicketServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reimburse</servlet-name>
        <url-pattern>/reimburse/*</url-pattern>
    </servlet-mapping>


</web-app>